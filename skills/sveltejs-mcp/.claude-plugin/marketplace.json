{
	"name": "svelte",
	"owner": {
		"name": "Svelte"
	},
	"plugins": [
		{
			"name": "svelte",
			"source": "./plugins/svelte",
			"description": "A plugin for all things Svelte development, MCP, skills, and more.",
			"lspServers": {
				"svelte": {
					"command": "svelteserver",
					"args": ["--stdio"],
					"extensionToLanguage": {
						".svelte": "svelte"
					}
				}
			}
		}
	]
}
